pid file = <%= @pid_file %>
uid = nobody
gid = nobody
use chroot = <%= @use_chroot %>
log format = %t %a %m %f %b
syslog facility = <%= @syslog_fac %>
<% if @log_file -%>
log file = <%= @log_file %>
<% end -%>
timeout = <%= @timeout %>
address = <%= @address %>
<% if @motd_file != 'UNSET' -%>
motd file = <%= @motd_file %>
<% end -%>
